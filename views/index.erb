  <!-- **********************************************************************************************************************************************************
      MAIN CONTENT
      *********************************************************************************************************************************************************** -->
  <!--main content start-->
  <section id="main-content">
    <section class="wrapper">
      <div class="row">
        <div class="col-lg-9">
          <! -- BASIC BUTTONS -->
          <div class="showback">
            <h4><i class="fa fa-angle-right"></i> Mode Control( now : <%= @mode %> )</h4>
            <button type="button" class="btn btn-success"><a class="white" href="/mode/main"> main  </a></button>
            <button type="button" class="btn btn-info"><a class="white" href="/mode/echo"> echo </a></button>
            <button type="button" class="btn btn-warning"><a class="white" href="/mode/presen"> presen </a></button>
            <button type="button" class="btn btn-danger"><a class="white" href="/mode/news"> news </a></button>
            <button type="button" class="btn btn-circle btn-default"><a class="black" href="/input"> next! </a></button>
          </div><!-- /showback -->
          <! -- BASIC BUTTONS -->
          <div class="showback">
            <h4><i class="fa fa-angle-right"></i> Motor Control( now : <%= @motorCommand %> )</h4>
            <button type="button" class="btn btn-success"><a class="white" href="/motor/go"> Go!!!  </a></button>
            <button type="button" class="btn btn-info"><a class="white" href="/motor/left"> Left </a></button>
            <button type="button" class="btn btn-warning"><a class="white" href="/motor/right"> Right</a></button>
            <button type="button" class="btn btn-danger"><a class="white" href="/motor/stop"> stop </a></button>
            <button type="button" class="btn btn-theme02"><a class="white" href="/motor/pp"> ﾊﾟﾀﾊﾟﾀ </a></button>
          </div><!-- /showback -->

          <! -- BUTTONS ROUND -->
          <div class="showback">
            <h4><i class="fa fa-angle-right"></i> Servo Control( now : <%= @servoCommand %> )</h4>
            <button type="button" class="btn btn-success"><a class="white" href="/servo/up">Up</a></button>
            <button type="button" class="btn btn-info"><a class="white" href="/servo/down">Down</a></button>
            <button type="button" class="btn btn-warning"><a class="white" href="/servo/pp">ﾊﾟﾀﾊﾟﾀ</a></button>
          </div><!-- /showback -->

          <! -- THEME BUTTONS -->
          <div class="showback">
            <h4><i class="fa fa-angle-right"></i> LED Control( now : <%= @ledCommand %> )</h4>
            <!-- Standard button -->
            <button type="button" class="btn btn-default"><a class="black" href="/led/white">White</a></button>
            <button type="button" class="btn btn-success"><a class="white" href="/led/green">Green</a></button>
            <button type="button" class="btn btn-primary"><a class="white" href="/led/blue">Blue</a></button>
            <button type="button" class="btn btn-info"><a class="white" href="/led/skyblue">SkyBlue</a></button>
            <button type="button" class="btn btn-warning"><a class="white" href="/led/yellow">Yellow</a></button>
            <button type="button" class="btn btn-danger"><a class="white" href="/led/red">Red</a></button>
            <button type="button" class="btn btn-theme02"><a class="white" href="/led/purple">Purple</a></button>
          </div><!-- /showback -->
          <div class="showback">
            <h4><i class="fa fa-angle-right"></i> Right LCD Control ( English Only )</h4>
            <form action="/rlcd" method="post">
              <input type="text" class="form-control" name="rightLcdString">
              <span class="help-block"> <%= @rightLcdString %></span>
            </form>
          </div><!-- /showback -->
          <div class="showback">
            <h4><i class="fa fa-angle-right"></i> Left LCD Control ( English Only )</h4>
            <form action="/llcd" method="post">
              <input type="text" class="form-control" name="leftLcdString">
              <span class="help-block"> <%= @leftLcdString %> </span>
            </form>
          </div><!-- /showback -->
          <div class="showback">
            <h4><i class="fa fa-angle-right"></i> Echo String Control ( Japanese OK )</h4>
            <form action="/echo" method="post">
              <input type="text" class="form-control" name="echoString">
              <span class="help-block"> <%= @echoString %> </span>
            </form>
          </div><!-- /showback -->
          <div class="showback">
            <h4><i class="fa fa-angle-right"></i> Speaking String Control ( Japanese OK )</h4>
            <form action="/speaking" method="post">
              <input type="text" class="form-control" name="speakingString">
              <span class="help-block"> <%= @speakingString %> </span>
            </form>
          </div><!-- /showback -->
        </div>


        <!-- **********************************************************************************************************************************************************
      RIGHT SIDEBAR CONTENT
      *********************************************************************************************************************************************************** -->

        <div class="col-lg-3 ds">
          <!--COMPLETED ACTIONS DONUTS CHART-->
          <h3>Actions</h3>
          <% settings.myaction.reverse_each do |action| %>
            <div class="desc">
            <div class="thumb">
              <span class="badge bg-theme"><i class="fa fa-clock-o"></i></span>
            </div>
            <div class="details">
              <p><muted><%= action[1].to_s %></muted><br/>
              <%= action[0] %><br/>
              </p>
            </div>
          </div>
        <% end %>
          <h3>Twitter</h3>
          <% @twitter.reverse_each do |tweet| %>
            <div class="desc">
            <div class="thumb">
              <div class="details">
                <%= tweet %><br/>
              </div>
            </div>
          </div>
        <% end %>
          <!-- CALENDAR-->
          <div id="calendar" class="mb">
            <div class="panel green-panel no-margin">
              <div class="panel-body">
                <div id="date-popover" class="popover top" style="cursor: pointer; disadding: block; margin-left: 33%; margin-top: -50px; width: 175px;">
                  <div class="arrow"></div>
                  <h3 class="popover-title" style="disadding: none;"></h3>
                  <div id="date-popover-content" class="popover-content"></div>
                </div>
                <div id="my-calendar"></div>
              </div>
            </div>
          </div><!-- / calendar -->
        </div><!-- /col-lg-3 -->
      </div><! --/row -->
    </section>
